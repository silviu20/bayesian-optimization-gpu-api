# API Configuration
API_KEY=123456789

# GPU Configuration
USE_GPU=true

# Logging Level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO

# CORS Settings (comma-separated list of origins)
ALLOWED_ORIGINS=http://localhost,http://localhost:8000

# Data Directory
DATA_DIR=/data

# Performance Settings
# Increase for more accurate but slower models
BOTORCH_N_RESTARTS=10
BOTORCH_N_RAW_SAMPLES=64

# PyTorch Settings
# Set to true to use single precision floating point
PYTORCH_SINGLE_PRECISION=false

# Number of worker threads for the API
WORKERS=1